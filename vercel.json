{
  "framework": "nextjs",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://iteshxt.me/:path*",
      "statusCode": 308,
      "has": [
        {
          "type": "host",
          "value": "www.iteshxt.me"
        }
      ]
    }
  ]
}
